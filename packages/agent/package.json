{
  "name": "@mindforge/agent",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "prompts"],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.28",
    "@langchain/core": "^0.3.75",
    "@langchain/google-genai": "^0.2.17",
    "@langchain/groq": "^0.2.4",
    "@langchain/langgraph": "^0.4.9",
    "@langchain/openai": "^0.6.12",
    "@mindforge/shared": "workspace:*",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.5.0"
  }
}
